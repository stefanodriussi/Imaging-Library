<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>2.0.0</Version>
        <Authors>Stefano Driussi</Authors>
        <Description>PCL Imaging Library for Document Scanning, contains Canny edge detection, Blob counter, quadrilateral transformation and many more.</Description>
        <PackageProjectUrl>https://github.com/stefanodriussi/Imaging-Library</PackageProjectUrl>
        <RepositoryUrl>https://github.com/stefanodriussi/Imaging-Library</RepositoryUrl>
        <PackageReleaseNotes>Forked from  https://github.com/enginkirmaci/Imaging-Library</PackageReleaseNotes>
        <PackageId>newimage.boundaries.detector</PackageId>
        <Product>Imaging Library 2 (netstandard)</Product>
        <AssemblyName>Imaging.Library2</AssemblyName>
        <RootNamespace>Imaging.Library</RootNamespace>
    </PropertyGroup>
</Project>